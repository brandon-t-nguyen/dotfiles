XTerm.vt100.reverseVideo: true

! URXvt general
URxvt.scrollBar: false
URxvt.perl-ext-common: default,matcher,selection-to-clipboard

URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! URxvt font
URxvt.font: xft:hack:size=9
!URxvt.font: xft:dejavusansmono:size=9

! URxvt colors
URxvt.foreground: 7 ! white
URxvt.background: 0 ! black

! normal
! black
URxvt.color0: #0d0d0d
! red
URxvt.color1: #cc0000
! green
URxvt.color2: #00cc00
! yellow
URxvt.color3: #cccc00
! blue
URxvt.color4: #0000cc
! magenta
URxvt.color5: #cc00cc
! cyan
URxvt.color6: #00cccc
! white
URxvt.color7: #cccccc

! bright
! black
URxvt.color8: #1a1a1a
! red
URxvt.color9: #ff3d3d
! green
URxvt.color10: #3dff3d
! yellow
URxvt.color11: #ffff3d
! blue
URxvt.color12: #3d3dff
! magenta
URxvt.color13: #ff3dff
! cyan
URxvt.color14: #3dffff
! white
URxvt.color15: #e6e6e6

!local configuration
