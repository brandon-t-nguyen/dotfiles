# Startup tasks

# Notification server
exec --no-startup-id dunst

# System monitor
exec --no-startup-id alacritty -t htop -e htop -t

# Switch to our first workspace
exec --no-startup-id i3-msg 'workspace 1:doc'
exec --no-startup-id i3-msg 'workspace 1:dev'


# Launch some applications
exec --no-startup-id picom -b
exec --no-startup-id udiskie
exec --no-startup-id nm-applet
